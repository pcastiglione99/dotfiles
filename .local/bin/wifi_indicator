#!/bin/bash
# Left click

[[ "${BLOCK_BUTTON}" -eq 1 ]] && networkmanager_dmenu -l 10

status=$(cat /sys/class/net/wlan0/operstate)

if [[ "${status}" == "up" ]]; then
    #quality=$(grep wlan0 /proc/net/wireless | awk '{ print int($3 * 100 / 70) }')
    ssid=$(iwgetid -r)
    echo " ${ssid} "
else
  echo " off " 
fi

